public static int birthdayCakeCandles(List<Integer> candles) {
    // Write your code here
        int count = 1;
        int maxele = 0;
        int n = candles.size();
        for(int i=0; i<n; i++){
            if(candles.get(i)==maxele){
                count++;
            }else if(candles.get(i)>maxele){
                maxele=candles.get(i);
                
            }
        }
        return count;
    }
